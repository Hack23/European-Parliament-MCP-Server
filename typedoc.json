{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "plugin": [
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-zod"
  ],
  "name": "European Parliament MCP Server API",
  "includeVersion": true,
  "readme": "README.md",
  "categorizeByGroup": true,
  "categoryOrder": [
    "MCP Tools - Core Data",
    "MCP Tools - Advanced Analysis",
    "MCP Tools - OSINT Intelligence",
    "MCP Tools - EP API v2 Data",
    "MCP Tools - EP API v2 Documents",
    "MCP Tools - EP API v2 Feed",
    "MCP Prompts",
    "MCP Resources",
    "EP API Client",
    "Generated Types",
    "Schemas",
    "Types",
    "Utils",
    "Tools",
    "Intelligence",
    "Business",
    "Marketing",
    "Clients",
    "*"
  ],
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": true,
  "hideGenerator": false,
  "sort": ["kind", "instance-first", "required-first", "alphabetical"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Enum",
    "Variable"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "visibilityFilters": {
    "protected": true,
    "private": true,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "cleanOutputDir": true,
  "hostedBaseUrl": "https://hack23.github.io/European-Parliament-MCP-Server/api/",
  "gitRevision": "main"
}
