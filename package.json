{
  "name": "european-parliament-mcp-server",
  "version": "0.0.1",
  "description": "Model Context Protocol server for European Parliament open data",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "european-parliament-mcp": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run src",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:performance": "vitest run tests/performance",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "test:coverage": "vitest run src --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=json --outputFile=coverage/junit.xml",
    "test:licenses": "license-compliance --direct --allow 'MIT;ISC;0BSD;BSD-2-Clause;BSD-3-Clause;Apache-2.0;Unlicense;CC0-1.0;BlueOak-1.0.0'",
    "knip": "knip",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run lint && npm run knip && npm run test:unit && npm run build",
    "prepack": "npm run build",
    "docs": "typedoc --out docs/api src/index.ts",
    "docs:clean": "rm -rf docs/api docs/coverage docs/test-results docs/e2e-results",
    "docs:coverage": "npm run test:coverage && mkdir -p docs/coverage && cp -r coverage/* docs/coverage/",
    "docs:test-reports": "mkdir -p docs/test-results && vitest run --reporter=html --reporter=json --outputFile=docs/test-results/results.json > docs/test-results/report.html || true",
    "docs:e2e-reports": "mkdir -p docs/e2e-results && vitest run tests/e2e --reporter=html --reporter=json --outputFile=docs/e2e-results/results.json > docs/e2e-results/report.html || true",
    "docs:build": "npm run docs:clean && npm run docs && npm run docs:coverage && npm run docs:test-reports && npm run docs:e2e-reports",
    "docs:sitemap": "node scripts/generate-sitemap.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "european-parliament",
    "open-data",
    "api",
    "typescript"
  ],
  "author": "Hack23 AB",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hack23/European-Parliament-MCP-Server.git"
  },
  "bugs": {
    "url": "https://github.com/Hack23/European-Parliament-MCP-Server/issues"
  },
  "homepage": "https://github.com/Hack23/European-Parliament-MCP-Server#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "SECURITY.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "lru-cache": "^11.0.2",
    "undici": "^7.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.3",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "knip": "^5.45.4",
    "license-compliance": "^3.0.1",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typedoc": "0.28.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.6"
  }
}
